    <services>
        <!-- Resource owners -->
        <service id="hwi_oauth.abstract_resource_ownermap" class="HWI\Bundle\OAuthBundle\Security\Http\ResourceOwnerMap" abstract="true">
            <argument key="$httpUtils" type="service" id="security.http_utils" />
            <argument key="$possibleResourceOwners">%hwi_oauth.resource_owners%</argument>
            <argument key="$resourceOwners" type="collection" /><!-- map -->
            <argument key="$locator" type="tagged_locator" tag="hwi_oauth.resource_owner" index-by="resource-name" />
        </service>

        <service id="hwi_oauth.resource_ownermap_locator" class="HWI\Bundle\OAuthBundle\Security\Http\ResourceOwnerMapLocator" />
    </services>
